# include <stdio.h> /*llama las los metodos a usar*/

int main(void) /*metodo principal*/
{
	
	printf("Mi nombre es Arturo Vel%csquez.",160); /*Imprime mi nombre %c es la designacion de caracter y 160 la a con acento*/
	printf("\nMi direccion de correo es: a.velasquez11\n"); /* imprme mi direccion*/
	return 0; /*Retorna un valor entero, 0 por convencion*/
}

